{
  "name": "takion-ocr-lambda",
  "version": "1.0.0",
  "description": "TakionOCR Lambda function for PDF OCR processing",
  "main": "dist/lambda-handler.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/local-runner.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "ocr-local": "ts-node src/local-runner.ts",
    "ocr-s3-local": "ts-node src/local-s3-runner.ts",
    "lambda-test": "ts-node src/local-s3-runner.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.400.0",
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-ssm": "^3.400.0",
    "@aws-sdk/client-textract": "^3.400.0",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "pdf2pic": "^3.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.0",
    "@types/pdf-parse": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "ocr",
    "pdf",
    "aws-lambda",
    "textract",
    "tesseract"
  ],
  "author": "Devin AI",
  "license": "MIT"
}
